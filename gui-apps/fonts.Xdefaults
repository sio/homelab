! Duplicate of fontconfig settings for applications
! without fontconfig support
!
! This file is meant to be placed to ~/.Xdefaults or ~/.Xresources
! Use `xrdb -query` to check whether it has been loaded

Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
